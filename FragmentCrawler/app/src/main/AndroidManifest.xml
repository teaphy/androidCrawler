<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.teaphy.fragmentcrawler">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.LazyFragment">
        <activity android:name=".lifecycle.HideOrShowActivity"></activity>
        <activity android:name=".lifecycle.LifecycleActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.StaticLoadActivity" />
        <activity android:name=".activity.StaticLoadMultiActivity" />
        <activity android:name=".activity.DynamicLoadActivity" />
        <activity android:name=".activity.DynamicLoadMultiActivity" />
        <activity android:name=".activity.DynamicLazyLoadMultiActivity" />
        <activity android:name=".activity.ViewPagerLoadMultiActivity" />
    </application>

</manifest>